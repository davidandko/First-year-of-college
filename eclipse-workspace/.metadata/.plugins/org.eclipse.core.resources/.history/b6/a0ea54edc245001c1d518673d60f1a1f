#include<stdio.h>
int cifri(int n){
	int cifra,cifra2;
	cifra=n%10;
	cifra2=(n/10)%10;
	if(n==0){
		return 1;
	}
	if((cifra%cifra2)==0){
		return 1;
	}
	else
		return 0;
	cifri(n/10);

}
int main(){
	int n;
	printf("Vnesete broj: \n");
	fflush(stdout);
	scanf("%d",&n);
	cifri(n);
	return 0;
}
