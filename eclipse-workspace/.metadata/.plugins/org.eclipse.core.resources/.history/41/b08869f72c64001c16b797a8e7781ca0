#include<stdio.h>
#include<string.h>
#include<ctype.h>
#define MAX 100

int main(){
	int i = 0;
	char a[MAX],ch;
	while((i<MAX-1) && ((ch=getchar())!='\n')){
	a[i++] = ch;
	a[i] = '\0';
	}
	printf("Vnesenata tekstualna niza e \"%s".\n",a);
	return 0;
}

