#include<stdio.h>

int cifri(int n){
	int cifra;
	cifra=n%10;

	if(n==0){
		return 0;
	}
	if((cifra%cifri(n/10))==0){
		return 1;
	}
	else
		return 0;

}
int main(){
	int n;
	printf("Vnesete broj: \n");
	fflush(stdout);
	scanf("%d",&n);
	cifri(n);
	return 0;
}
