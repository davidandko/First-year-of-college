#include<stdio.h>
int proverka(int n){
	int cifra;
	cifra=n%10;
	if(n==0){
		return 0;
	}
	if(cifra%((proverka(n/10))%10))
		return 1;

}
int main(){
	int n;
	printf("Vnesete broj: \n");
	fflush(stdout);
	scanf("%d",&n);
	proverka(n);
}
