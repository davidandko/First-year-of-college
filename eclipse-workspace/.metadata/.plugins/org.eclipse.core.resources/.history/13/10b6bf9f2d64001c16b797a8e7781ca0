#include<stdio.h>
#include<string.h>
#include<ctype.h>
#define MAX 100

int main(){
	int i = 0;
	char a[MAX],b[MAX],ch,ch1;
	while((i<MAX-1) && ((ch=getchar())!='\n')){
	a[i++] = ch;
	a[i] = '\0';
	}
	printf("Vnesenata tekstualna niza e \"%s\".\n",a);
	while((i<MAX-1) && ((ch1=getchar())!='\n')){
		b[i++] = ch1;
		b[i] = '\0';
		}
	printf("Vnesenata tekstualna niza e \"%s\".\n",b);

	return 0;
}

