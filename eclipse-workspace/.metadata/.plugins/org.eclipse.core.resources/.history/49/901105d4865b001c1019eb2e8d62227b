#include<stdio.h>
#define MAX 100
int prost(int n){
	int i;
for(i=2;i<(n/2);i++){
	if(n%i==0){
		return 0; //brojot e slozen
	}
	else{
		return 1; //brojot e prost
	}
}
}
int main(){
	int i,pom,a[MAX],b[MAX],n;
		printf("Vnesete golemina na nizata\n");
		fflush(stdout);

		scanf("%d",&n);

		printf("Vnesuvajte elementi vo nizata\n");
		fflush(stdout);

		for(i=0;i<n;i++){
			scanf("%d",&a[i]);
		}
		for(i=0;i<n;i++){
			if((prost(a[i]) == 1)&&(prost(a[i+1] == 1))){
				pom=a[i];
				a[i]=a[i+1];
				a[i+1]=pom;
				i++;
			}
			for(j=0;j<n;j++){
						b[j]=a[i];
						printf(" %d ",b[j]);
					}
		}


return 0;
}


