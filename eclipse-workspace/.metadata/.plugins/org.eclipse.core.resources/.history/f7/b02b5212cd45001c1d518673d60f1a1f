#include<stdio.h>
int proverka(int n){
	if(((proverka(n/10))%(proverka((n/10)%10)))==0){

		return 1;
	}
	else
		return 0;
	proverka(n/10);
}
int main(){
	int n;
	printf("Vnesete broj: \n");
	fflush(stdout);
	scanf("%d",&n);
	proverka(n);
}
