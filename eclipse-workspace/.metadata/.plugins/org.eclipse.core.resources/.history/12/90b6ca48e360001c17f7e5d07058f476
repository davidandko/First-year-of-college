#include<stdio.h>
#define MAX 100

int prevrti(int a[],int n){
	int i,*temp;
	for(i=0;i<n;i++){
		*temp=*(a+i);
		*(a+i)=*(a+n-1-i);
		*(a+n-1-i)=*(a+i);
}
for(i=0;i<n;i++){
	printf("%d ",*(a+i));
}
return 0;
}
int main(){
	int i,n;
	int a[MAX];
	printf("Vnesete broj na elementi: \n");
	fflush(stdout);
	scanf("%d",&n);

	printf("Vnesete clenovi vo nizata: \n");
	fflush(stdout);

	for(i=0;i<n;i++){
		scanf("%d",a+i);
	}
	prevrti(a,n);
	return 0;
}
