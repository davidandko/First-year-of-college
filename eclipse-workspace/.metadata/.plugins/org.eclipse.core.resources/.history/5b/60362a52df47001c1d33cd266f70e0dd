#include<stdio.h>
#include<math.h>
int transformacija(int n){

	int broj,cifra,stepen=1;
while(n>0){
	while(n>0){
		stepen++;
		n/=10;
	}
	cifra=n%10;
	broj=pow(10,stepen)*cifra;
	n/=10;
}
return broj;
}
int main(){

	int n;
	printf("Vnesi broj: ");
	fflush(stdout);
	scanf("%d",&n);
	printf("\nBrojot posle transformacija e: %d",transformacija(n));

	return 0;
}

