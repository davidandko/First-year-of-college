#include<stdio.h>
#include<math.h>
#define MAX 100
int prost(int n){
	int i;
for(i=2;i<=sqrt(n);i++){
	if(n%i==0)
		return 0; // brojot e slozen
}
return 1;
}
int main(){

	int i,pom,a[MAX],b[MAX],n;
	printf("Vnesete golemina na nizata\n");
	fflush(stdout);

	scanf("%d",&n);

	printf("Vnesuvajte elementi vo nizata\n");
	fflush(stdout);

	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<n-1;i++){
		if((prost(a[i]) == 1)&&(prost(a[i+1])== 1)){
			pom=a[i];
			a[i]=a[i+1];
			a[i+1]=pom;
			i++;
		}
	}
for(i=0;i<n;i++){
	b[i]=a[i];
	printf(" %d ",b[i]);
}

return 0;
}


